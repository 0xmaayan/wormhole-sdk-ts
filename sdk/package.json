    "@wormhole-foundation/sdk-base": "file:../core/base",
    "@wormhole-foundation/sdk-connect": "file:../connect",
    "@wormhole-foundation/sdk-cosmwasm": "file:../platforms/cosmwasm",
    "@wormhole-foundation/sdk-cosmwasm-core": "file:../platforms/cosmwasm/protocols/core",
    "@wormhole-foundation/sdk-cosmwasm-ibc": "file:../platforms/cosmwasm/protocols/ibc",
    "@wormhole-foundation/sdk-cosmwasm-tokenbridge": "file:../platforms/cosmwasm/protocols/tokenBridge",
    "@wormhole-foundation/sdk-definitions": "file:../core/definitions",
    "@wormhole-foundation/sdk-evm": "file:../platforms/evm",
    "@wormhole-foundation/sdk-evm-cctp": "file:../platforms/evm/protocols/cctp",
    "@wormhole-foundation/sdk-evm-core": "file:../platforms/evm/protocols/core",
    "@wormhole-foundation/sdk-evm-portico": "file:../platforms/evm/protocols/portico",
    "@wormhole-foundation/sdk-evm-tokenbridge": "file:../platforms/evm/protocols/tokenBridge",
    "@wormhole-foundation/sdk-solana": "file:../platforms/solana",
    "@wormhole-foundation/sdk-solana-cctp": "file:../platforms/solana/protocols/cctp",
