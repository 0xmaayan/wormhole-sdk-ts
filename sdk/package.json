{
  "name": "@wormhole-foundation/sdk",
  "version": "0.6.6-beta.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wormhole-foundation/connect-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/wormhole-foundation/connect-sdk/issues"
  },
  "homepage": "https://github.com/wormhole-foundation/connect-sdk#readme",
  "directories": {
    "test": "__tests__"
  },
  "license": "Apache-2.0",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "module": "./dist/esm/index.js",
  "description": "The core package for the Connect SDK, used in conjunction with 1 or more of the chain packages",
  "files": [
    "dist/esm",
    "dist/cjs"
  ],
  "keywords": [
    "wormhole",
    "sdk",
    "typescript",
    "connect"
  ],
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false,
  "scripts": {
    "build:cjs": "tsc -p ./tsconfig.cjs.json  && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "build:esm": "tsc -p ./tsconfig.esm.json",
    "build": "npm run build:esm && npm run build:cjs",
    "rebuild": "npm run clean && npm run build",
    "clean": "rm -rf ./dist && rm -rf ./.turbo",
    "lint": "npm run prettier && eslint --fix ./src --ext .ts",
    "prettier": "prettier --write ./src",
    "coverage": "jest --config ./jest.config.ts --coverage",
    "docs": "typedoc"
  },
  "exports": {
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/cjs/*.d.ts",
        "default": "./dist/cjs/*.js"
      },
      "import": {
        "types": "./dist/esm/*.d.ts",
        "default": "./dist/esm/*.js"
      }
    }
  },
  "typesVersions": {
    "*": {
      "algorand": [
        "dist/cjs/algorand.d.ts",
        "dist/esm/algorand.d.ts"
      ],
      "evm": [
        "dist/cjs/evm.d.ts",
        "dist/esm/evm.d.ts"
      ],
      "solana": [
        "dist/cjs/solana.d.ts",
        "dist/esm/solana.d.ts"
      ],
      "cosmwasm": [
        "dist/cjs/cosmwasm.d.ts",
        "dist/esm/cosmwasm.d.ts"
      ],
      "aptos": [
        "dist/cjs/aptos.d.ts",
        "dist/esm/aptos.d.ts"
      ],
      "sui": [
        "dist/cjs/sui.d.ts",
        "dist/esm/sui.d.ts"
      ],
      "platforms/algorand": [
        "dist/cjs/platforms/algorand.d.ts",
        "dist/esm/platforms/algorand.d.ts"
      ],
      "platforms/evm": [
        "dist/cjs/platforms/evm.d.ts",
        "dist/esm/platforms/evm.d.ts"
      ],
      "platforms/solana": [
        "dist/cjs/platforms/solana.d.ts",
        "dist/esm/platforms/solana.d.ts"
      ],
      "platforms/cosmwasm": [
        "dist/cjs/platforms/cosmwasm.d.ts",
        "dist/esm/platforms/cosmwasm.d.ts"
      ],
      "platforms/aptos": [
        "dist/cjs/platforms/aptos.d.ts",
        "dist/esm/platforms/aptos.d.ts"
      ],
      "platforms/sui": [
        "dist/cjs/platforms/sui.d.ts",
        "dist/esm/platforms/sui.d.ts"
      ],
      "*": [
        "dist/cjs/index.d.ts",
        "dist/esm/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@wormhole-foundation/sdk-base": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-definitions": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-connect": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-evm": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-evm-core": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-evm-tokenbridge": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-evm-portico": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-evm-cctp": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-solana": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-solana-core": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-solana-tokenbridge": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-solana-cctp": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-cosmwasm": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-cosmwasm-core": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-cosmwasm-tokenbridge": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-cosmwasm-ibc": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-sui": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-sui-core": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-sui-tokenbridge": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-aptos": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-aptos-core": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-aptos-tokenbridge": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-algorand": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-algorand-core": "0.6.6-beta.1",
    "@wormhole-foundation/sdk-algorand-tokenbridge": "0.6.6-beta.1"
  },
  "type": "module"
}